# auto detect text files
* text=auto

# force all text file types to have Unix line endings
*.* text eol=lf

# files that are truly binary and should not be modified
*.7z binary
*.ai binary
*.bin binary
*.bmp binary
*.com binary
*.data binary
*.dds binary
*.dll binary
*.dmg binary
*.eof binary
*.eot binary
*.exe binary
*.flv binary
*.gif binary
*.gz binary
